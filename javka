local Section = Tab:NewSection("Humanoid Scripts")
Section:NewSlider("WalkSpeed", "Change ur WalkSpeed!", 250, 16, function(sp) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = sp
end)
Section:NewSlider("JumpHeight", "Change ur JumpHeight!", 500, 0, function(jh) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpHeight = jh
end)
Section:NewSlider("HipHeight", "Change ur HipHeight!", 500, 2, function(hh) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.HipHeight = hh
end)

local Tab = Window:NewTab("Credits")
local Section = Tab:NewSection("Credits:")
Section:NewLabel("the ka#6246: Kadium Owner")
Section:NewLabel("Mage Accelerator#8589: Helping With Scripts")
Section:NewLabel("Nexo Animator: Used To Make Scripts")
Section:NewButton("Discord Server", "Copys discord server invite link!", function()
    setclipboard("dsc.gg/kadium")
    toclipboard("dsc.gg/kadium")
end)
local Tab = Window:NewTab("Settings")
local Section = Tab:NewSection("Settings:")
Section:NewKeybind("UI Keybind", "Change UI keybind", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)
Section:NewButton("Rejoin", "Rejoins The Server!", function()
    local tpservice= game:GetService("TeleportService")
    local plr = game.Players.LocalPlayer
    tpservice:Teleport(game.PlaceId, plr)
end)
local Section = Tab:NewSection("Theme Changing:")
for theme, color in pairs(colors) do
    Section:NewColorPicker(theme, "Change your "..theme, color, function(color3)
        Library:ChangeColor(theme, color3)
    end)
end
